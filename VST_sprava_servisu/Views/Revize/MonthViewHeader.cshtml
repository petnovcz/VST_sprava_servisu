            </div>
            
        </div>

        <div class="item @if (ViewBag.NextYear == ViewBag.Rok) { @X; }">
            <div class="col-lg-offset-2 col-lg-8">
                <div class="col-lg-12" style="text-align:center">
                    <h3>@ViewBag.NextYear</h3>
                </div>
                <div class="col-lg-1">
                    @Html.ActionLink("1", "Nahled", "Revize", new { Rok = ViewBag.NextYear, Mesic = 1 }, new { @class = "btn btn-block btn-success" })
