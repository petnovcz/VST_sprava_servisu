
@{
    ViewBag.Title = "MonthViewHeader";
}
@{ Layout = null;}
@{ var X= "active";}

<div id="myCarousel" class="carousel" data-ride="carousel" data-interval="false" >
    <!-- Indicators -->
    
    <!-- Wrapper for slides -->
    <div class="carousel-inner" >
        <div class="item @if (ViewBag.ThisYear == ViewBag.Rok) { @X; }" >
            
            <div class="col-lg-offset-2 col-lg-8">
                <div class="col-lg-12" style="text-align:center">
                    <h3>@ViewBag.ThisYear</h3>
                </div>
                <div class="col-lg-1">
                    @Html.ActionLink("1","Nahled","Revize", new { Rok = ViewBag.ThisYear, Mesic = 1},new { @class = "btn btn-block btn-success"})
                </div>
                <div class="col-lg-1">
                    @Html.ActionLink("2", "Nahled", "Revize", new { Rok = ViewBag.ThisYear, Mesic = 2 }, new { @class = "btn btn-block btn-success" })
                </div>
                <div class="col-lg-1">
