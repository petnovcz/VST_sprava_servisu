                </div>
                <div class="col-lg-1">
                    @Html.ActionLink("7", "Nahled", "Revize", new { Rok = ViewBag.ThisYear, Mesic = 7 }, new { @class = "btn btn-block btn-success" })
                </div>
                <div class="col-lg-1">
                    @Html.ActionLink("8", "Nahled", "Revize", new { Rok = ViewBag.ThisYear, Mesic = 8 }, new { @class = "btn btn-block btn-success" })
                </div>
                <div class="col-lg-1">
