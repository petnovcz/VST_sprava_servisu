@model IEnumerable<VST_sprava_servisu.KategoriePoruchy>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NazevKategorie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OpravnenaReklamace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NeopravnenaReklamace)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReklamaceServisniZasah)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NazevKategorie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OpravnenaReklamace)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NeopravnenaReklamace)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReklamaceServisniZasah)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
